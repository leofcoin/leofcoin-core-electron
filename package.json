{
  "name": "leofcoin-core-electron",
  "version": "0.1.4",
  "description": "Leofcoin Desktop App.",
  "main": "app/index.js",
  "repository": "https://github.com/leofcoin/leofcoin-core-electron.git",
  "author": "VandeurenGlenn <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "flat": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron app/index.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "production": "node .build.js && npm run make && node .clean.js",
    "publish": "electron-forge publish",
    "lint": "eslint src --color",
    "build": "electron-builder",
    "release": "build"
  },
  "build": {
    "productName": "Leofcoin",
    "copyright": "Copyright Â© 2018 VandeurenGlenn <vandeurenglenn@gmail.com>",
    "appId": "leofcoin.desktop.core",
    "directories": {
      "app": "app"
    },
    "mac": {
      "category": "your.app.category.type"
    },
    "asar": false,
    "asarUnpack": [
      "node_modules/**/**"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": {
        "target": "AppImage",
        "arch": [
          "armv7l"
        ]
      }
    }
  },
  "backed": {
    "bundles": [
      {
        "src": "src/index.js",
        "dest": "app/index.js",
        "moduleName": "leofcoin",
        "format": "cjs"
      },
      {
        "src": "src/renderer.js",
        "dest": "app/renderer.js",
        "moduleName": "leofcoinRenderer",
        "format": "iife"
      },
      {
        "src": "src/sections/miner.js",
        "dest": "app/sections/miner.js",
        "moduleName": "coreMiner",
        "format": "iife"
      },
      {
        "src": "src/sections/explorer.js",
        "dest": "app/sections/explorer.js",
        "moduleName": "coreExplorer",
        "format": "iife"
      },
      {
        "src": "src/sections/terminal.js",
        "dest": "app/sections/terminal.js",
        "moduleName": "coreTerminal",
        "format": "iife",
        "plugins": {
          "node-resolve": {},
          "commonjs": {}
        }
      },
      {
        "src": "src/sections/wallet/wallet.js",
        "dest": "app/sections/wallet.js",
        "moduleName": "walletEl",
        "format": "iife"
      }
    ]
  },
  "devDependencies": {
    "electron": "^1.8.6",
    "electron-builder": "^19.56.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "rollup": "^0.59.1",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "dependencies": {
    "backed": "^0.13.0",
    "bs58": "^4.0.1",
    "crypto-io-fs": "^0.1.2",
    "crypto-js": "^3.1.9-1",
    "custom-button": "^0.11.1",
    "custom-html-tag": "^0.1.6",
    "custom-pages": "^0.12.0",
    "custom-renderer": "^0.1.3",
    "custom-renderer-mixin": "^0.1.10",
    "custom-select-mixins": "^0.11.2",
    "custom-svg-icon": "^0.9.0-preview.1",
    "custom-svg-iconset": "^0.9.1",
    "custom-tabs": "^0.11.0",
    "go-ipfs-dep": "^0.4.14",
    "install": "^0.11.0",
    "ipfs-bitswap": "^0.20.0",
    "ipfs-block-service": "^0.14.0",
    "ipfsd-ctl": "^0.32.1",
    "leofcoin-core": "^0.1.0-beta.1",
    "multi-wallet": "^0.1.1",
    "socket-request": "^0.1.0",
    "socket-request-client": "^0.1.5",
    "tiny-worker": "^2.1.2",
    "webworker": "^0.8.4",
    "xterm": "^3.2.0"
  },
  "resolutions": {
    "backed": "0.11.1",
